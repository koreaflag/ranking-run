    <array>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>app.runcrew</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>exp+runcrew</string>
        </array>
      </dict>
      <dict>
        <key>CFBundleURLSchemes</key>
        <array>
          <string>kakaoYOUR_KAKAO_NATIVE_APP_KEY</string>
        </array>
      </dict>
